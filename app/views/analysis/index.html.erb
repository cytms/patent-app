<% content_for :stylesheet do %>
	<%= stylesheet_link_tag    "map", :media => "all" %>
<% end %>

<div class="page-header">
	<h3><i class="icon-picture"></i> Map Visualization </h3>
	<blockquote>Self-organizing Map <span class="label label-info">In progress...</span></blockquote>
</div>

<div class="row">
	<div class="span12">
		<div class="map-wrapper">
			<div class="map-content" id="1">
				<h3>Samsung's 1000 patents in 2007 
					<small><br>32 x 24 matrix (Smoothed Data Histograms)</small>
				</h3>
				<img class="well well-small" src="/assets/map/sominfo.jpg">
				<div class="row">
					<div class="map thumbnail pull-left">
						<div class="map-img">
						</div>
					</div>
					<div class="map-info well well-small pull-right">

						<p><code>Learning Rate : 0.75</code></p>
						<p><code>Number of Iterations : 1000</code></p>
					</div>
				</div>
			</div>
			<hr>

		</div>
	</div>
</div>

<% content_for :javascript do %>
	<%= javascript_include_tag "map" %> 
<% end %>